; top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 2
max_line_length = 120

[*.{js,jsx}]
quote_type = single

[*.{json,html}]
quote_type = double

[{package.json,.babelrc}]
indent_size = 2
